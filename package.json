{
  "name": "@folio/inn-reach",
  "version": "1.1.0",
  "description": "Description for inn-reach",
  "main": "src/index.js",
  "repository": "",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=12.20.1"
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./output",
    "test-snapshot-load": "stripes serve --coverage --port 3001 --okapi https://folio-snapshot-load-okapi.dev.folio.org & pid=$! && cypress run; status=$?; kill $pid; echo Exiting with status $status; exit $status",
    "test": "stripes serve --coverage --port 3001 --okapi http://localhost:3002 & pid1=$! && env LOGCAT=startup yakbak-proxy -i -x -n & pid2=$! && wait-on http://localhost:3001 && cypress run; kill $pid1 $pid2",
    "regenerate": "stripes serve --port 3001 --okapi http://localhost:3002 & pid1=$! && rm -rf tapes && env LOGCAT=startup yakbak-proxy -i -x https://folio-snapshot-load-okapi.dev.folio.org & pid2=$! && wait-on http://localhost:3001 && cypress run; kill $pid1 $pid2",
    "coverage-summary": "nyc report --reporter=text-summary",
    "coverage": "nyc report --reporter=text",
    "lsof": "lsof -P -i :3001 -i :3002 || true",
    "kill": "yarn lsof | awk '{print $2}' | egrep '^[0-9]+$' | xargs kill",
    "test:jest": "jest --ci --coverage",
    "lintfix": "eslint ./ --fix && stylelint \"src/**/*.css\" --fix",
    "lint": "eslint ./ && stylelint \"src/**/*.css\"",
    "eslint": "eslint ./",
    "stylelint": "stylelint \"src/**/*.css\"",
    "formatjs-compile": "formatjs compile-folder --ast --format simple ./translations/ui-inn-reach ./translations/ui-inn-reach/compiled"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-react": "^7.10.4",
    "@cypress/code-coverage": "^3.9.6",
    "@folio/eslint-config-stripes": "^5.2.0",
    "@folio/stripes": "^7.0.0",
    "@folio/stripes-cli": "^1.20.0 || ^2.0.0",
    "@folio/stripes-core": "^8.0.0",
    "@formatjs/cli": "^4.2.10",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^11.0.2",
    "@testing-library/react-hooks": "^5.1.2",
    "@testing-library/user-event": "^12.1.10",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.3.0",
    "history": "^5.0.0",
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.6.1",
    "cypress": "^7.4.0",
    "eslint": "^6.2.1",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-jest": "^24.1.3",
    "identity-obj-proxy": "^3.0.0",
    "inflected": "^2.0.4",
    "jest": "^26.4.2",
    "jest-junit": "^12.0.0",
    "localforage": "^1.9.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.8.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "regenerator-runtime": "^0.13.3",
    "sinon": "^7.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-junit-formatter": "^0.2.2",
    "wait-on": "^6.0.0",
    "yakbak-proxy": "^1.7.0"
  },
  "dependencies": {
    "@folio/stripes-data-transfer-components": "^4.1.0",
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0",
    "react-final-form": "^6.3.0",
    "react-final-form-arrays": "^3.1.0",
    "uuid": "^3.0.2"
  },
  "peerDependencies": {
    "@folio/stripes": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.8.0",
    "react-router": "*",
    "react-router-dom": "^5.2.0",
    "query-string": "^5.0.0"
  },
  "optionalDependencies": {
    "@folio/plugin-find-user": "^5.0.1"
  },
  "stripes": {
    "actsAs": [
      "app",
      "settings"
    ],
    "icons": [
      {
        "name": "app",
        "alt": "Inn-reach management app",
        "title": "Inn-reach"
      }
    ],
    "displayName": "ui-inn-reach.meta.title",
    "route": "/innreach",
    "okapiInterfaces": {
      "inn-reach": "1.0",
      "loan-types": "2.0",
      "statistical-code-types": "1.0",
      "statistical-codes": "1.0",
      "location-units": "2.0",
      "users": "15.0"
    },
    "permissionSets": [
      {
        "permissionName": "module.inn-reach.enabled",
        "displayName": "UI: ui-inn-reach module is enabled",
        "visible": true
      },
      {
        "permissionName": "settings.inn-reach.enabled",
        "displayName": "Settings (inn-reach): display list of settings pages",
        "subPermissions": [
          "settings.enabled"
        ],
        "visible": true
      }
    ]
  }
}
